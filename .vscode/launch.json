{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Chrome",
      "port": 9222,
      "request": "attach",
      "type": "chrome",
      "webRoot": "${workspaceFolder}"
    },

    {
      "type": "node",
      "name": "Launch",
      "request": "launch",
      "program": "${workspaceFolder}/cmd/dependabot",
      "envFile": "${workspaceFolder}/.env",
      "args": ["update", "go_modules", "dependabot/cli"]
    }
  ]
}
